<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity"
    android:background="@color/background1">

    <!-- Warnings -->
	<LinearLayout
	    android:id="@+id/warnings"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:padding="10dp"
	    android:background="@color/background2"
	    android:orientation="vertical" >
	    <!-- Clickable title -->
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:clickable="true"
		    android:text="Warnings" />
	</LinearLayout>
	<LinearLayout android:id="@+id/warnings_text"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:background="@android:color/holo_purple">
		<!-- Content -->
		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:text="@string/hello_world" />
		
		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:text="@string/hello_world" />
		
		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:text="@string/hello_world" />
		
		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:text="@string/hello_world" />
		
		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:text="@string/hello_world" />
		
		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:text="@string/hello_world" />
		
		
		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:text="@string/hello_world" />
	</LinearLayout>

	<!-- Job Set up -->
	<LinearLayout
	    android:id="@+id/jobSetUp"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:padding="10dp"
	    android:background="@color/background2"
	    android:orientation="vertical" >
	    <!-- Clickable title -->
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:clickable="true"
		    android:text="Job Set up" />
	</LinearLayout>
	<LinearLayout android:id="@+id/jobSetUp_text"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:background="@android:color/holo_purple">
		<!-- Content -->
		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:text="@string/hello_world" />
	</LinearLayout>
	
	<!-- Procedure -->
	<LinearLayout
	    android:id="@+id/procedure"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:padding="10dp"
	    android:background="@color/background2"
	    android:orientation="vertical" >
	    <!-- Clickable title -->
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:clickable="true"
		    android:text="Procedure" />
	</LinearLayout>
	<LinearLayout android:id="@+id/procedure_text"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:background="@android:color/holo_purple">
		<!-- Content -->
		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:text="@string/hello_world" />
	</LinearLayout>
	
	<!-- Close up -->
	<LinearLayout
	    android:id="@+id/closeUp"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:padding="10dp"
	    android:background="@color/background2"
	    android:orientation="vertical" >
	    <!-- Clickable title -->
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:clickable="true"
		    android:text="Close up" />
	</LinearLayout>
	<LinearLayout android:id="@+id/closeUp_text"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:background="@android:color/holo_purple">
		<!-- Content -->
		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:text="@string/hello_world" />
	</LinearLayout>
	
	<!-- Tools -->
	<LinearLayout
	    android:id="@+id/tools"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:padding="10dp"
	    android:background="@color/background2"
	    android:orientation="vertical" >
	    <!-- Clickable title -->
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:clickable="true"
		    android:text="Tools" />
	</LinearLayout>
	<LinearLayout android:id="@+id/tools_text"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:background="@android:color/holo_purple">
		<!-- Content -->
		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:text="@string/hello_world" />
	</LinearLayout>
	
	<!-- Pictures -->
	<LinearLayout
	    android:id="@+id/pictures"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:padding="10dp"
	    android:background="@color/background2"
	    android:orientation="vertical" >
	    <!-- Clickable title -->
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:clickable="true"
		    android:text="Pictures" />
	</LinearLayout>
	<LinearLayout android:id="@+id/pictures_text"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:background="@android:color/holo_purple">
		<!-- Content -->
		<TextView
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		    android:text="@string/hello_world" />
	</LinearLayout>
</LinearLayout>
